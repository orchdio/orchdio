                    },
                    "cover": {
                      "type": "string",
                      "example": "https://resources.tidal.com/images/123456789/640x640.jpg"
                    }
                  }
                },
                "ytmusic": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "example": "123456789"
                    },
                    "title": {
                      "type": "string",
                      "example": "The Less I Know The Better"
                    },
                    "artists": {
                      "type": "array",
                      "example": [
                        "Tame Impala"
                      ]
                    },
                    "album": {
                      "type": "string",
