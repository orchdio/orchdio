        }
      },
      "TrackPayload": {
        "description": "A track payload",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "platforms": {
                  "$ref": "#/components/responses/PlatformsPayload"
                },
                "entity": {
                  "type": "string",
                  "example": "track"
                },
                "short_url": {
                  "type": "string",
                  "example": "TzrWfvoQ"
                }
              }
            }
          }
        }
      },
      "PlaylistPayload": {
        "description": "The payload for a playlist",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "meta": {
                  "type": "object",
                  "properties": {
                    "length": {
                      "type": "string",
                      "description": "Duration of the playlist, formatted as HH:MM:SS",
                      "example": "01:23:45"
                    },
                    "title": {
                      "type": "string",
                      "example": "My Playlist"
                    },
                    "owner": {
                      "type": "string",
                      "example": "Obsidian Lover"
                    },
                    "url": {
                      "type": "string",
                      "example": "https://open.spotify.com/playlist/123456789"
                    },
                    "cover": {
                      "type": "string",
                      "example": "https://i.scdn.co/image/123456789"
                    },
                    "short_url": {
                      "type": "string",
                      "example": "TzrWfvoQ"
                    }
                  }
                },
                "platforms": {
                  "$ref": "#/components/responses/PlatformsPayload"
                },
                "task_id": {
                  "type": "string",
                  "example": "123456789"
                },
                "entity": {
                  "type": "string",
                  "example": "playlist"
                }
              }
            }
          }
        }
      },

      "CreateNewAppResponse": {
        "description": "The response for creating a new app",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "string",
                  "format": "uuid",
